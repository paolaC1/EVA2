    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;Ingresa tu calificaci&#243;n&quot;" newline="True"/>
            <declare name="ical" type="Integer" array="False" size=""/>
            <input variable="ical"/>
            <if expression="ical == 100">
                <then>
                    <output expression="&quot;A&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="ical == 90">
                        <then>
